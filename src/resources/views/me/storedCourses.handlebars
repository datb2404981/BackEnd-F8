<div class="mt-4">
  <h3>Khóa Học Của Tui</h3>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Stt</th>
        <th scope="col">Tên khóa học</th>
        <th scope="col">Trình độ</th>
        <th scope="col" colspan="4">Thời gian tạo</th>
      </tr>
    </thead>
  {{#each courses}}
    <tbody>
      <tr>
        <th scope="row">{{sum @index 1}}</th>
        <td>{{this.name}}</td>
        <td>{{this.level}}</td>
        <td>{{this.createdAt}}</td>
        <td>
          <button type="button" class="btn btn-success" onclick="window.location.href='/courses/{{this._id}}/edit'">Edit</button>        </td>
        <td>
          <button type="button" href="" class="btn btn-danger">Delete</button>
        </td>
      </tr>

      </tr>
    </tbody>
  {{/each}}
  </table>
</div>